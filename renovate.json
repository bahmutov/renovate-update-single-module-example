{
  "extends": [
    "config:base",
    "github>teppeis/renovate-config"
  ],
  "enabledManagers": ["npm"],
  "automerge": false,
  "schedule": ["at any time"],
  "timezone": "America/New_York",
  "lockFileMaintenance": {
    "enabled": true
  },
  "separatePatchReleases": true,
  "separateMultipleMajor": true,
  "masterIssue": true,
  "labels": [
    "type: dependencies",
    "renovate"
  ],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "excludePackagePatterns": ["debug"],
      "enabled": false
    },
    {
      "packagePatterns": ["^f", "^night"],
      "enabled": true
    },
    {
      "packagePatterns": ["*"],
      "enabled": true
    }
  ]
}
