{
  "extends": [
    "config:base"
  ],
  "enabledManagers": ["npm"],
  "automerge": false,
  "timezone": "America/New_York",
  "lockFileMaintenance": {
    "enabled": true
  },
  "separatePatchReleases": true,
  "separateMultipleMajor": true,
  "masterIssue": true,
  "labels": [
    "type: dependencies",
    "renovate"
  ],
  "packageRules": [
    {
      "packageNames": ["debug"],
      "enabled": true,
      "updateTypes": ["major", "minor", "patch"],
      "rangeStrategy": "bump"
    },
    {
      "packagePatterns": ["*"],
      "enabled": false
    }
  ]
}
